void main() {
  int n = 10; // Número de termos desejado (pode ser substituído)

  int a = 0;
  int b = 1;

  print('Sequência de Fibonacci até o $n-ésimo termo:');
  print(a); // Primeiro termo
  print(b); // Segundo termo

  for (int i = 2; i < n; i++) {
    int nextTerm = a + b;
    print(nextTerm);
    a = b;
    b = nextTerm;
  }
}
